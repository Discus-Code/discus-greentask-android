<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/settingsAlarmFragment"
        android:name="org.discusinstitute.greentaskandroid.discus.fragments.SettingsAlarmFragment"
        android:label="fragment_settings_alarm"
        tools:layout="@layout/fragment_settings_alarm" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="org.discusinstitute.greentaskandroid.discus.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_settingsAlarmFragment"
            app:destination="@id/settingsAlarmFragment" />
        <action
            android:id="@+id/action_homeFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment><action android:id="@+id/action_global_homeFragment" app:destination="@id/homeFragment"/>
    <fragment
        android:id="@+id/detailFragment"
        android:name="org.discusinstitute.greentaskandroid.discus.fragments.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" /><action android:id="@+id/action_global_detailFragment" app:destination="@id/detailFragment"/>
</navigation>